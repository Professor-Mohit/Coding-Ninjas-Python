   "source": [
    "Write a Program to find the factorial of a given number using loop."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 30,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "720\n"
     ]
    }
   ],
   "source": [
    "n = int(input())\n",
    "product = 1\n",
    "i = 0\n",
    "\n",
    "for i in range (1, n+1):\n",
    "    product =  product * i\n",
    "print(product)\n",
    "\n",
    "# in while loop\n",
    "\n",
    "n = int(input())\n",
    "f = 1\n",
    "i = 1\n",
    "\n",
    "while i <= n:\n",
    "    f *= i\n",
    "    i += 1\n",
    "print(f)"
   ]
  },
