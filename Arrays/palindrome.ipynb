   "source": [
    "nums  = list(map(int, input().split(\" \")))\n",
    "p = True\n",
    "for i in range(len(nums)//2):\n",
    "    if nums[i] != nums[-i - 1]:\n",
    "        p = False\n",
    "        break\n",
    "if p:\n",
    "    print(\"Palindrome\" , nums)\n",
    "else:\n",
    "    print(\"not a palindrome\" , nums)\n",
    "\n"
   ]
  }
