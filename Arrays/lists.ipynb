   "source": [
    "# Maximum marks list\n",
    "\n",
    "all_marks = list(map(int, input().split(\" \")))\n",
    "# taking list of integer as input\n",
    "# converting string to integer using map function\n",
    "max_marks = all_marks[0]\n",
    "for marks in all_marks:\n",
    "    max_marks = max(marks, max_marks)\n",
    "print(max_marks)"
   ]
  }
