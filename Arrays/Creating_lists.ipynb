    "print(\"Total 1:\" , total)\n",
    "\n",
    "print(\"adding again\")\n",
    "\n",
    "total2 = 0\n",
    "for i in range(len(marks)):\n",
    "    total2 += marks[i]\n",
    "print(\"Total 2: \",total2)"
